{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --config .eslintrc.cjs --cache --ext .ts,.js,.vue,.yaml,.json . && npm run lint:prettier",
    "lint:fix": "eslint --config .eslintrc.cjs --cache --ext .ts,.js,.vue,.yaml,.json --fix . && prettier --write --cache --list-different .",
    "lint:prettier": "prettier --check --cache .",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "@nuxt/types": "^2.18.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxt/ui": "^3.1.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-yml": "^1.18.0",
    "nuxt": "^3.17.4",
    "prettier": "^3.5.3",
    "uuid": "^11.1.0",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "vue-tsc": "^2.2.10"
  }
}
